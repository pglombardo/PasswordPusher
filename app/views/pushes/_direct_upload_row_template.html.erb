<%# Template for one direct-upload progress row. JS clones this on direct-upload:initialize and sets id, progress bar id, file name. %>
<template id="direct-upload-row-template">
  <li class="list-group-item list-group-item-primary small">
    <div class="progress" style="height: 1.5rem">
      <div class="progress-bar progress-bar-striped progress-bar-animated direct-row-progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%"></div>
    </div>
  </li>
</template>
